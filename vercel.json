{
  "crons": [
    {
      "path": "/api/sync/google-drive",
      "schedule": "0 2 * * *"
    }
  ],
  "functions": {
    "src/app/api/sync/google-drive/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/admin/usability-tests/route.ts": {
      "maxDuration": 60
    }
  },
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "1",
      "NEXT_TELEMETRY_DISABLED": "1",
      "NODE_OPTIONS": "--max-old-space-size=2048",
      "PRISMA_GENERATE_SKIP_AUTOINSTALL": "true",
      "PRISMA_CLI_BINARY_TARGETS": "linux-musl",
      "RESEND_API_KEY": "re_build_placeholder_key_123"
    }
  },
  "installCommand": "npm ci --prefer-offline --no-audit --no-fund",
  "buildCommand": "npx prisma generate --no-engine && npm run build",
  "outputDirectory": ".next"
}
