{
  "crons": [
    {
      "path": "/api/cron/expire-appointments",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/backup-analytics",
      "schedule": "0 2 1 * *"
    },
    {
      "path": "/api/cron/check-stripe-webhook",
      "schedule": "0 */6 * * *"
    }
  ],
  "redirects": [
    {
      "source": "/:path((?!api/webhooks/).*)",
      "has": [
        {
          "type": "host",
          "value": "nest-haus.at"
        }
      ],
      "destination": "https://www.nest-haus.at/:path*",
      "permanent": true
    },
    {
      "source": "/:path((?!api/webhooks/).*)",
      "has": [
        {
          "type": "host",
          "value": "da-hoam.at"
        }
      ],
      "destination": "https://www.da-hoam.at/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/webhooks/stripe",
      "destination": "/api/webhooks/stripe"
    }
  ],
  "headers": [
    {
      "source": "/api/webhooks/stripe",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        }
      ]
    }
  ]
}
