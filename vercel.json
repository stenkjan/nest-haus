{
  "crons": [
    {
      "path": "/api/cron/expire-appointments",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/backup-analytics",
      "schedule": "0 2 1 * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/webhooks/stripe",
      "destination": "/api/webhooks/stripe"
    }
  ],
  "headers": [
    {
      "source": "/api/webhooks/stripe",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        }
      ]
    }
  ]
}
