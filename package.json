{
  "name": "nest-haus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/start-dev.js",
    "dev:local": "next dev --hostname localhost",
    "dev:network": "node scripts/start-dev.js",
    "dev:all": "next dev --hostname 0.0.0.0",
    "build": "npx prisma generate && next build",
    "build:windows": "taskkill /f /im node.exe 2>nul & timeout /t 2 & npx prisma generate && next build",
    "postinstall": "npx prisma generate",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:performance": "vitest run src/test/performance",
    "test:compliance": "vitest run src/test/compliance",
    "test:components": "vitest run src/**/__tests__",
    "test:stores": "vitest run src/store/__tests__",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "typecheck": "tsc --noEmit",
    "test:typecheck": "npm run typecheck",
    "test:all": "npm run test:typecheck && npm run test:run",
    "test:critical": "npm run test:compliance && npm run test:performance && npm run test:stores",
    "db:seed": "tsx prisma/seed.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "@prisma/nextjs-monorepo-workaround-plugin": "^6.15.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/jspdf": "^1.3.3",
    "@types/node-cron": "^3.0.11",
    "@upstash/redis": "^1.35.0",
    "@vercel/blob": "^1.1.1",
    "@vitest/ui": "^3.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.17.0",
    "googleapis": "^150.0.1",
    "html2canvas": "^1.4.1",
    "isomorphic-dompurify": "^2.26.0",
    "jsdom": "^26.1.0",
    "jspdf": "^3.0.2",
    "motion": "^12.18.1",
    "next": "^15.3.3",
    "node-cron": "^4.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "resend": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "vitest": "^3.2.4",
    "whatwg-fetch": "^3.6.20",
    "zod": "^3.25.76",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "node-fetch": "^2.7.0",
    "prisma": "^6.15.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.2",
    "typescript": "^5"
  }
}
